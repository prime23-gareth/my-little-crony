version: "3.9"
services:

  loanrangers:
    container_name: loanrangers
    image: theloanrangers
    restart: unless-stopped
    ports:
      - "3838:3838"

  nginx:
    container_name: nginx
    image: nginx:latest
    restart: unless-stopped
    ports:
      - "80:80"
      - "443:443"
    volumes:
      - ./nginx/nginx.conf:/etc/nginx/nginx.conf